<table border="1">
    <thead>
        <tr>
            <th>ID</th>
            <th>File Name</th>
            <th>Download Link</th>
            <th>Created At</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% history.forEach(pdf => { %>
        <tr>
            <td><%= pdf.id %></td>
            <td><%= pdf.file_name %></td>
            <td><a href="/<%= pdf.file_path %>" download><%= pdf.file_name %></a></td>
            <td><%= pdf.created_at %></td>
            <td>
                <form action="/delete-history/<%= pdf.id %>" method="post" style="display: inline;">
                    <button type="submit" onclick="return confirm('Are you sure you want to delete this history entry?')">
                        Delete
                    </button>
                </form>
            </td>
        </tr>
        <% }); %>
    </tbody>
</table>
<a href="/">Back to Home</a>

